<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hfuu.web.entity.AdminEntity" table="admin" schema="hfuutest">
        <id name="adminId">
            <column name="adminId" sql-type="int(11)"/>
        </id>
        <property name="adminNum">
            <column name="adminNum" sql-type="char(10)" length="10"/>
        </property>
        <property name="adminName">
            <column name="adminName" sql-type="varchar(8)" length="8"/>
        </property>
        <property name="adminPw">
            <column name="adminPw" sql-type="varchar(20)" length="20"/>
        </property>
        <property name="adminSex">
            <column name="adminSex" sql-type="char(2)" length="2" not-null="true"/>
        </property>
        <property name="adminPhone">
            <column name="adminPhone" sql-type="char(11)" length="11" not-null="true"/>
        </property>
        <property name="adminAvatar">
            <column name="adminAvatar" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>