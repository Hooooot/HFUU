<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hfuu.web.entity.CourseEntity" table="course" schema="hfuutest">
        <id name="cozId">
            <column name="cozId" sql-type="int(11)"/>
        </id>
        <property name="cozNum">
            <column name="cozNum" sql-type="char(9)" length="9" not-null="true"/>
        </property>
        <property name="cozName">
            <column name="cozName" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="classNum">
            <column name="classNum" sql-type="char(7)" length="7" not-null="true"/>
        </property>
        <property name="tcNum">
            <column name="tcNum" sql-type="char(10)" length="10" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>