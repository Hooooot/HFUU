<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hfuu.web.entity.DepartmentEntity" table="department" schema="hfuutest">
        <id name="depId">
            <column name="depId" sql-type="int(11)"/>
        </id>
        <property name="depNum">
            <column name="depNum" sql-type="char(2)" length="2" not-null="true"/>
        </property>
        <property name="depName">
            <column name="depName" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>