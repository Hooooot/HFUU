<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hfuu.web.entity.TaskEntity" table="task" schema="hfuutest">
        <id name="taskId">
            <column name="taskId" sql-type="int(11)"/>
        </id>
        <property name="taskName">
            <column name="taskName" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <property name="taskDesc">
            <column name="taskDesc" sql-type="varchar(128)" length="128" not-null="true"/>
        </property>
        <property name="cozNum">
            <column name="cozNum" sql-type="char(9)" length="9" not-null="true"/>
        </property>
        <property name="tcNum">
            <column name="tcNum" sql-type="char(10)" length="10" not-null="true"/>
        </property>
        <property name="pubTime">
            <column name="pubTime" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="deadline">
            <column name="deadline" sql-type="timestamp" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>