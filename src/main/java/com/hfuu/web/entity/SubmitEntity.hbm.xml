<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hfuu.web.entity.SubmitEntity" table="submit" schema="hfuutest">
        <id name="subId">
            <column name="subId" sql-type="int(11)"/>
        </id>
        <property name="subTime">
            <column name="subTime" sql-type="timestamp"/>
        </property>
        <property name="subState">
            <column name="subState" sql-type="char(6)" length="6" not-null="true"/>
        </property>
        <property name="subFile">
            <column name="subFile" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
        <many-to-one name="stuNum" class="com.hfuu.web.entity.StudentEntity">
            <column name="stuNum" not-null="true"/>
        </many-to-one>
        <many-to-one name="taskId" class="com.hfuu.web.entity.TaskEntity">
            <column name="taskId"/>
        </many-to-one>
    </class>
</hibernate-mapping>