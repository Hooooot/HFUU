<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hfuu.web.entity.SubmitEntity" table="submit" schema="hfuutest">
        <id name="subId">
            <column name="subId" sql-type="int(11)"/>
        </id>
        <property name="taskId">
            <column name="taskId" sql-type="int(11)" not-null="true"/>
        </property>
        <property name="stuNum">
            <column name="stuNum" sql-type="char(10)" length="10" not-null="true"/>
        </property>
        <property name="subTime">
            <column name="subTime" sql-type="timestamp" not-null="true"/>
        </property>
        <property name="subState">
            <column name="subState" sql-type="char(6)" length="6" not-null="true"/>
        </property>
        <property name="subFile">
            <column name="subFile" sql-type="varchar(64)" length="64" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>