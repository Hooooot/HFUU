<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.hfuu.web.entity.SubmitEntity" table="submit" schema="hfuutest">
        <id name="subId" column="subId"/>
        <property name="subTime" column="subTime"/>
        <property name="subState" column="subState"/>
        <property name="subFile" column="subFile"/>
    </class>
</hibernate-mapping>